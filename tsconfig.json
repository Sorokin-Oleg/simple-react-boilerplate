{
  "compilerOptions": {
    "jsx": "react",
    "module": "ESNext",
    "moduleResolution": "Node",
    "noImplicitAny": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "sourceMap": true,
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
  },
  "include": [
    "source/**/*"
  ]
}
